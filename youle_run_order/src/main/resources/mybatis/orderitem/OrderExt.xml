<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sioeye.youle.run.order.gateways.repository.dao.OrderDao">
  <select id="getOrderByUser" parameterType="java.lang.String" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List" />
    from "order"
    where objectid = #{objectid,jdbcType=VARCHAR} and usersid = #{userid,jdbcType=VARCHAR}
  </select>
</mapper>